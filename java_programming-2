# Write a program to read the marks of 5 subjects through the keyboard Find out the aggregate and percentage of marks obtained by the student Assume maximum marks
that can be obtained by a student in each subject as 100.

  #java programming
package ra;
import java.util.Scanner;

public class marks {
	public static void calculations(int sub1,int sub2,int sub3,int sub4,int sub5) {
		 float subj1=sub1;
		System.out.println("The percentage of Subject 1:"+subj1+"%");
		float subj2=sub2;
		System.out.println("The percentage of Subject 2:"+subj2+"%");
		float subj3=sub3;
		System.out.println("The percentage of Subject 3:"+subj3+"%");
		float subj4=sub4;
		System.out.println("The percentage of Subject 4:"+subj4+"%");
		float subj5=sub5;
		System.out.println("The percentage of Subject 5:"+subj5+"%");
		System.out.println("The overall percentage for 5 subjects:"+(sub1+sub2+sub3+sub4+sub5));
		
	}

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("\nenter the Subject 1 marks:");
		int sub1=sc.nextInt();
		System.out.println("\nenter the Subject 2 marks:");
		int sub2=sc.nextInt();
		System.out.println("\nenter the Subject 3 marks:");
		int sub3=sc.nextInt();
		System.out.println("\nenter the Subject 4 marks:");
		int sub4=sc.nextInt();
		System.out.println("\nenter the Subject 5 marks:");
		int sub5=sc.nextInt();
		calculations(sub1 ,sub2,sub3,sub4,sub5);
		
	}

}
